{
  "port": 5000,
  "address": "0.0.0.0",
  "title": "My Script Server",

  "auth": {
    "type": "htpasswd",
    "htpasswd_path": "/app/conf/.htpasswd"
  },

  "access": {
    "allowed_users": [ "admin" ],
    "admin_users": [ "admin" ]
  },

  "logging": {
    "execution_file": "$DATE-$ID.log",
    "execution_date_format": "%y-%m-%d_%H-%M"
  },

  "security": {
    "xsrf_protection": "token"
  }
}
